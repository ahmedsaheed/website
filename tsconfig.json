{
    "compilerOptions": {
        "jsx": "preserve",
        "types": ["node"],
        "target": "es2016",
        "module": "commonjs" ,
        "esModuleInterop": true ,
        "forceConsistentCasingInFileNames": true ,
        "strict": true ,
        "skipLibCheck": true,
        "lib": ["dom", "dom.iterable", "esnext"],
        "allowJs": true,
        "noEmit": true,
        "incremental": true,
        "resolveJsonModule": true,
        "isolatedModules": true
    },
    "include": ["next-env.d.ts", "**/*.ts", "**/*.tsx", "component/theme.js", "pages/books/api.js"],
    "exclude": ["node_modules"]
}
